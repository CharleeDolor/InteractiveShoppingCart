{"ast":null,"code":"// @ is an alias to /src\nimport ShopItem from '@/components/ShopItem.vue';\nimport ActionButton from '@/components/ActionButton.vue';\nexport default {\n  name: 'ShoppingCart',\n  components: {\n    ShopItem,\n    ActionButton\n  },\n  data() {\n    return {\n      items: []\n    };\n  },\n  beforeMount() {\n    this.items = this.$addedToCart;\n  },\n  computed: {\n    totalPrice() {\n      let total = 0;\n      this.items.forEach(item => {\n        total += item.price * item.quantity;\n      });\n      return total;\n    }\n  }\n};","map":{"version":3,"names":["ShopItem","ActionButton","name","components","data","items","beforeMount","$addedToCart","computed","totalPrice","total","forEach","item","price","quantity"],"sources":["C:\\Users\\User\\Desktop\\InteractiveShoppingCart\\src\\components\\ShoppingCart.vue"],"sourcesContent":["<template>\r\n  <h1>Cart Items:</h1>\r\n  <div class=\"item-page\">\r\n    <table class=\"item-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Product Name</th>\r\n          <th>Price</th>\r\n          <th>Quantity</th>\r\n          <th>Action</th>\r\n        </tr>\r\n      </thead>\r\n\r\n      <tbody v-for=\"(item, index) in this.$addedToCart\" v-bind:key=\"index\">\r\n        <tr>\r\n          <ShopItem :item=item></ShopItem>\r\n          <td>{{ item.quantity }}</td>\r\n\r\n          <td class=\"button-group\">\r\n            <ActionButton prod=\"Update Quantity\" :item=item :index=index :list=this.items></ActionButton>\r\n            <ActionButton prod=\"Remove\" :item=item :index=index :list=this.items></ActionButton>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n\r\n      <div>\r\n        <h3>Total: {{ this.totalPrice }}</h3>\r\n      </div>\r\n\r\n\r\n    </table>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport ShopItem from '@/components/ShopItem.vue';\r\nimport ActionButton from '@/components/ActionButton.vue';\r\n\r\nexport default {\r\n  name: 'ShoppingCart',\r\n\r\n  components:{\r\n    ShopItem,\r\n    ActionButton\r\n  },\r\n  data() {\r\n    return {\r\n      items: [],\r\n    }\r\n  },\r\n\r\n  beforeMount() {\r\n    this.items = this.$addedToCart;\r\n  },\r\n\r\n  computed: {\r\n    totalPrice() {\r\n      let total = 0;\r\n      this.items.forEach(item => {\r\n        total += item.price * item.quantity;\r\n      });\r\n\r\n      return total;\r\n    },\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.button-group{\r\n  display: flex;\r\n  gap: 0.5rem;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\nbutton{\r\n    font-size: 20px\r\n}\r\n.item-page {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n</style>"],"mappings":"AAoCA;AACA,OAAOA,QAAO,MAAO,2BAA2B;AAChD,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,IAAI,EAAE,cAAc;EAEpBC,UAAU,EAAC;IACTH,QAAQ;IACRC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EAEDC,WAAWA,CAAA,EAAG;IACZ,IAAI,CAACD,KAAI,GAAI,IAAI,CAACE,YAAY;EAChC,CAAC;EAEDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,IAAIC,KAAI,GAAI,CAAC;MACb,IAAI,CAACL,KAAK,CAACM,OAAO,CAACC,IAAG,IAAK;QACzBF,KAAI,IAAKE,IAAI,CAACC,KAAI,GAAID,IAAI,CAACE,QAAQ;MACrC,CAAC,CAAC;MAEF,OAAOJ,KAAK;IACd;EAEF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}